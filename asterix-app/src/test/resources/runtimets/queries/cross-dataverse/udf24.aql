// Returns java.lang.ClassCastException

drop dataverse test if exists;
create dataverse test;

write output to nc1:"rttest/cross-dataverse_udf24.adm";

create function test.myRangeFn($n)
{
   range(1,$n)
}

for $i in test.myRangeFn(100)
return $i
