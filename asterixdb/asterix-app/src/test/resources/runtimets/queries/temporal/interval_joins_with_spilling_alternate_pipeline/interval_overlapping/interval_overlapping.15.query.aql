/*
 * Description      :   Check temporal join functionality when spilling for interval
 * Expected Result  :   Success
 * Date             :   11th Jul, 2017
 */

use dataverse CostModel;

count(
  for $r in dataset R
    for $s in dataset S
    where /*+ disjoint-interval-partition-join [0,257,513,769,1025] */ interval-overlapping($r.data, $s.data)
    return $r.filler
);
